{
  "name": "web-coding-boilerplate-pug",
  "version": "1.0.0",
  "private": true,
  "author": "BLY PROJECT",
  "browserslist": [
    "last 2 version",
    "> 1%"
  ],
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "dev": "webpack --config webpack.dev.js --env development",
    "release": "webpack --config webpack.prod.js --env production",
    "lint": "run-s lint-*",
    "lint-js": "eslint ./src/**/*.js",
    "lint-scss": "stylelint ./src/**/*.scss",
    "lint-html": "markuplint **/*.pug -p",
    "fix": "run-s fix-*",
    "fix-js": "eslint ./src/**/*.js --fix",
    "fix-scss": "stylelint ./src/**/*.scss --fix",
    "fix-html": "echo \"\\033[31m不具合があるので使用不可。手動で修正しましょう。\\033[00m\"",
    "archive": "gda --"
  },
  "devDependencies": {
    "@markuplint/pug-parser": "2.2.5",
    "autoprefixer": "10.4.19",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.11.0",
    "css-minimizer-webpack-plugin": "4.2.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-webpack-plugin": "3.2.0",
    "file-loader": "6.2.0",
    "git-diff-archive": "1.2.0",
    "glob": "8.1.0",
    "html-loader": "3.1.2",
    "html-webpack-plugin": "5.6.0",
    "image-webpack-loader": "8.1.0",
    "markuplint": "2.11.4",
    "mini-css-extract-plugin": "2.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-cli": "^10.1.0",
    "postcss-loader": "7.3.4",
    "postcss-sort-media-queries": "5.2.0",
    "prettier": "2.8.8",
    "pug-html-loader": "1.1.5",
    "puglint-webpack-plugin": "1.0.3",
    "sass": "1.75.0",
    "sass-loader": "13.3.3",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.7.0",
    "stylelint-webpack-plugin": "3.3.0",
    "terser-webpack-plugin": "5.3.10",
    "webpack": "5.91.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.15.2",
    "webpack-merge": "5.10.0"
  },
  "dependencies": {
    "jquery": "3.7.1",
    "jquery.easing": "1.4.1",
    "normalize.css": "^8.0.1",
    "slick-carousel": "1.8.1",
    "swiper": "11.2.5"
  }
}
